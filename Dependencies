            else:
                raise NoViableAltException(self)

        except RecognitionException as re:
            localctx.exception = re
            self._errHandler.reportError(self, re)
            self._errHandler.recover(self, re)
            if isinstance( re, NoViableAltException ):
                    raise NoViableAltException(self)